my_dict = {"name":"javier","role":"Manager"}
for k in my_dict:
    print(k) #por la palabra del diccionario
    print(my_dict[k]) #por el significado de la palabra

print("--------------ejercicio 2--------------------")
dict2 = {"nombre":"zabaleta","edad":"47"}
for key in dict2.keys():
    print(key)
for x in dict2.values():
    print(x)
for key, x in dict2.items():
    print(key,"=",x)     